<div class="container-fluid">
  <div class="row">
    <div class="col-2">
      <%= render "admin/sidebar" %>
    </div>
    <div class="col">
      <%= form_for @menu, url: admin_menu_path(@menu), multipart: true, html: { method: :patch } do |f| %>
        <%= render partial: 'form', locals: {f: f, menu: @menu} %>
      <% end %>
    </div>
  </div>
</div>