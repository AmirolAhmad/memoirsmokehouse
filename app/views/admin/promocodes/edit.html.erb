<div class="container-fluid">
  <div class="row">
    <div class="col-lg-2 col-md-4 col-sm-12">
      <%= render "admin/sidebar" %>
    </div>
    <div class="col-lg-10 col-md-8 col-sm-12">
      <%= form_for @promocode, url: admin_promocode_path(@promocode), multipart: true, html: { method: :patch } do |f| %>
        <%= render partial: 'form', locals: {f: f, promocode: @promocode} %>
      <% end %>
    </div>
  </div>
</div>